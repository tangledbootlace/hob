# Local Development Environment Variables
# Copy this file to .env and update the values as needed

# Database Configuration
SA_PASSWORD=Password123

# RabbitMQ Configuration
RABBITMQ_USER=local
RABBITMQ_PASSWORD=local

# Grafana Configuration
GRAFANA_ADMIN_PASSWORD=grafana

# API Configuration
ASPNETCORE_ENVIRONMENT=Development
HOB_SERVICES_ContainerPort=8080

# OpenTelemetry Configuration
OPENTELEMETRY_SERVICE_NAME=HOB.API
OPENTELEMETRY_JAEGER_ENDPOINT=http://jaeger:4317

# Connection Strings
CONNECTION_STRING_DB=Server=db;Database=HOB;User Id=sa;Password=Password123;TrustServerCertificate=True;MultipleActiveResultSets=True;
CONNECTION_STRING_RABBITMQ=amqp://local:local@rabbitmq:5672

# Report Settings
REPORT_OUTPUT_DIRECTORY=/reports

# Dashboard Configuration
NODE_ENV=production
NEXT_PUBLIC_API_URL=http://hob.api:8080
